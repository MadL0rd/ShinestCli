{
  "name": "@madl0rd/shinest-cli",
  "version": "0.1.8",
  "description": "CLI for ShinestBot envieronment",
  "author": "MadL0rd",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadL0rd/ShinestCli"
  },
  "homepage": "https://github.com/MadL0rd/ShinestCli",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "bin": {
    "shinest": "dist/main.js"
  },
  "scripts": {
    "build": "nest build && tsc-alias",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start -- g",
    "start:dev": "nest start --watch -- g",
    "start:debug": "nest start --debug --watch --",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@nestjs/common": "^11.0.0",
    "nest-commander": "^3.0.0",
    "ts-morph": "^26.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.3.1",
    "@types/node-fetch": "^2.6.11",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.2.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
