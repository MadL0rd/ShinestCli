{
  "name": "@madl0rd/shinest-cli",
  "version": "0.1.6",
  "description": "CLI for ShinestBot envieronment",
  "author": "MadL0rd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadL0rd/ShinestCli"
  },
  "homepage": "https://github.com/MadL0rd/ShinestCli",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "bin": {
    "shinest": "dist/main.js"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start -- g",
    "start:dev": "nest start --watch -- g",
    "start:debug": "env-cmd -f ./.env nest start --debug --watch -- g",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "nest-commander": "^3.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.12.7",
    "@types/node-fetch": "^2.6.11",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}